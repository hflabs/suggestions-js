        "esbuild": "^0.23.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.3",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-sonarjs": "^0.19.0",
        "jsdom": "^24.1.1",
        "prettier": "^3.5.3",
        "typescript": "^5.5.3",
        "vite": "^5.4.5",
